<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote fetch="https://github.com/aws4embeddedlinux"                  name="amazon"       />
  <remote fetch="https://github.com/jonmason/"                          name="arm"          />
  <remote fetch="https://github.com/linux4sam/"                         name="atmel"        />
  <remote fetch="https://github.com/beagleboard/"                       name="beagle"       />
  <remote fetch="https://github.com/emagii/"                            name="emagii"       />
  <remote fetch="https://github.com/kraj/"                              name="kraj"         />
  <remote fetch="https://github.com/LairdCP/"                           name="laird"        />
  <remote fetch="https://github.com/MarkusVolk/"                        name="mvolk"        />
  <remote fetch="https://git.openembedded.org/"                         name="oe"           />
  <remote fetch="https://github.com/OSSystems/"                         name="oss"          />
  <remote fetch="https://github.com/meta-rust/"                         name="rust"         />
  <remote fetch="https://github.com/sbabic/"                            name="sbabic"       />
  <remote fetch="https://github.com/Xilinx/"                            name="xilinx"       />
  <remote fetch="https://github.com/meta-qt5/"                          name="qt5"          />
  <remote fetch="https://git.yoctoproject.org/"                         name="yocto"        />


  <default sync-j="1"/>

  <project name="poky"                  path="poky"                          remote="yocto"    revision="kirkstone"          />
  <project name="meta-arm"              path="poky/meta-arm"                 remote="arm"      revision="kirkstone"          />
  <project name="meta-atmel"            path="poky/meta-atmel"               remote="atmel"    revision="kirkstone"          />
  <project name="meta-aws"              path="poky/meta-aws"                 remote="amazon"   revision="kirkstone"          />
  <project name="meta-binutils"         path="poky/meta-binutils"            remote="emagii"   revision="kirkstone"          />
  <project name="meta-browser"          path="poky/meta-browser"             remote="oss"      revision="master"             />
  <project name="meta-clang"            path="poky/meta-clang"               remote="kraj"     revision="kirkstone"          />
  <project name="meta-emagii"           path="poky/meta-emagii"              remote="emagii"   revision="kirkstone">
    <copyfile dest="oe-init-emagii-sdk" src="scripts/oe-init-emagii-sdk"  />
  </project>
  <project name="meta-intel"            path="poky/meta-intel"               remote="yocto"    revision="kirkstone"          />
  <project name="meta-java"             path="poky/meta-java"                remote="yocto"    revision="kirkstone"          />
  <project name="meta-mingw"            path="poky/meta-mingw"               remote="yocto"    revision="kirkstone"          />
  <project name="meta-openembedded"     path="poky/meta-openembedded"        remote="oe"       revision="kirkstone"          />
  <project name="meta-python2"          path="poky/meta-python2"             remote="oe"       revision="kirkstone"          />
  <project name="meta-qt5"              path="poky/meta-qt5"                 remote="qt5"      revision="kirkstone"          />
  <project name="meta-raspberrypi"      path="poky/meta-raspberrypi"         remote="yocto"    revision="kirkstone"          />
  <project name="meta-rpi64"            path="poky/meta-rpi64"               remote="emagii"   revision="kirkstone"          />
  <project name="meta-security"         path="poky/meta-security"            remote="yocto"    revision="kirkstone"          />
  <project name="meta-swupdate"         path="poky/meta-swupdate"            remote="sbabic"   revision="kirkstone"          />
  <project name="meta-ti"               path="poky/meta-ti"                  remote="yocto"    revision="kirkstone"          />
  <project name="meta-virtualization"   path="poky/meta-virtualization"      remote="yocto"    revision="kirkstone"          />
  <project name="meta-wayland"          path="poky/meta-wayland"             remote="mvolk"    revision="kirkstone"          />
  <project name="meta-map-sheriffco"    path="poky/meta-map-sheriffco"       remote="emagii"   revision="kirkstone"          />
  <project name="meta-laird-cp"         path="poky/meta-laird-cp"            remote="laird"    revision="lrd-10.0.0.x"       />
  <project name="meta-rust"             path="poky/meta-rust"                remote="rust"     revision="master"             />
</manifest>
